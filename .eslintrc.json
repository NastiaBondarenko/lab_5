{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules":{
        "eqeqeq": "off",
        "indent": [2, "tab"],
        "no-tabs": 0
    }, 
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ]
}
